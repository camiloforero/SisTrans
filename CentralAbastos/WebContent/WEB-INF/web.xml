 </servlet>
 <servlet>
  <display-name>ServletBusquedaPedidos</display-name>
  <servlet-name>ServletBusquedaPedidos</servlet-name>
  <servlet-class>co.edu.uniandes.centralAbastos.servlets.ServletBusquedaPedidos</servlet-class>
 </servlet>
 <servlet>
  <display-name>ServletResultadoBusquedaPedidos</display-name>
