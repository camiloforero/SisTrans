<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>CentralAbastos</display-name>
 <!-- =======-=======-=======-=======-=======- Servlets -=======-=======-=======-=======-======= -->
 <servlet>
  <display-name>ServletPlantilla</display-name>
  <servlet-name>ServletPlantilla</servlet-name>
  <servlet-class>co.edu.uniandes.centralAbastos.servlets.ServletPlantilla</servlet-class>
 </servlet>
 <servlet>
  <display-name>ServletResultadoBusquedaProductos</display-name>
