 </servlet>
 <!--===XxX===xXx===XxX===xXx===== Servlet Mappings =====xXx===XxX===xXx===XxX===-->
 <servlet-mapping>
  <servlet-name>ServletPlantilla</servlet-name>
  <url-pattern>/videosdefault.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletResultadoBusquedaProductos</servlet-name>
  <url-pattern>/resultadoBusquedaProductos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletBusquedaProductos</servlet-name>
  <url-pattern>/busquedaProductos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletBusquedaPedidos</servlet-name>
  <url-pattern>/busquedaPedidos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletResultadoBusquedaPedidos</servlet-name>
  <url-pattern>/resultadoBusquedaPedidos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletMenuUsuario</servlet-name>
  <url-pattern>/menuUsuario.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletRegistrarPedido</servlet-name>
  <url-pattern>/registrarPedido.htm</url-pattern>
